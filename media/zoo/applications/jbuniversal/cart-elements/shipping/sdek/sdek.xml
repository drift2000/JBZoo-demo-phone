<element type="element" group="Core" hidden="false">
    <name>JBZOO_ELEMENT_SHIPPING_SDEK_NAME</name>
    <author>JBZoo.com</author>
    <creationDate>2014</creationDate>
    <copyright>Copyright (C) JBZoo.com</copyright>
    <authorEmail>admin@jbzoo.com</authorEmail>
    <authorUrl>http://jbzoo.com</authorUrl>
    <version>1.0</version>
    <description>JBZOO_ELEMENT_SHIPPING_SDEK_DESC</description>

    <params>

        <param name="@sdek" type="jbspacer" default="JBZOO_ELEMENT_SHIPPING_SDEK_NAME" />

        <param name="from" type="sdekcityselect" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_CITY"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_CITY_DESC"/>
        <param name="login" type="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_LOGIN"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_LOGIN_DESC"/>
        <param name="password" type="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_PASSWORD"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_PASSWORD_DESC"/>
        <param name="key" type="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_YANDEX"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_YANDEX_DESC"/>
        <param name="rate" type="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ADD"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ADD_DESC"/>
        <param name="free_courier" type="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_FREE"
               description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_FREE_DESC"/>

        <param name="@source" type="jbspacer" default="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SEPARATOR_SOURCE" />

        <param name="source" type="radio" default="price" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SOURCE" description="">
            <option value="price">JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SOURCE_PRICE</option>
            <option value="elements">JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SOURCE_ELEMENTS</option>
        </param>

        <param name="@elements" type="jbspacer" default="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SEPARATOR_ELEMENTS" />

        <param name="element_weight" type="jbelementlist" types="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_WEIGHT"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_WEIGHT_DESC"/>
        <param name="element_length" type="jbelementlist" types="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_LENGTH"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_LENGTH_DESC"/>
        <param name="element_width" type="jbelementlist" types="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_WIDTH"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_WIDTH_DESC"/>
        <param name="element_height" type="jbelementlist" types="text" default="" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_HEIGHT"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_ELEMENTS_HEIGHT_DESC"/>

        <param name="@defaults" type="jbspacer" default="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_SEPARATOR_DEFAULT" />

        <param name="default_weight" type="text" default="1" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_WEIGHT"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_WEIGHT_DESC"/>
        <param name="default_length" type="text" default="10" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_LENGTH"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_LENGTH_DESC"/>
        <param name="default_width" type="text" default="10" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_WIDTH"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_WIDTH_DESC"/>
        <param name="default_height" type="text" default="20" label="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_HEIGHT"
                description="JBZOO_ELEMENT_SHIPPING_SDEK_PARAM_DEFAULT_HEIGHT_DESC"/>

    </params>

</element>
